{% extends 'base.html.twig' %} {% block title %}Client{% endblock %} {% block body %}
{% block erreur %}{{erreur}} {% endblock %}
<div class="page-wrapper">
    <div class="page-container">
        <div class="main-content">
            <div class="section__content section__content--p30">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="card">
                                <div class="card-header">
                                    <h1>Client</h1>

                                    <table class="table">
                                        <tbody>
                                            <tr>
                                                <th>Id</th>
                                                <td>{{ client.id }}</td>
                                            </tr>
                                            <tr>
                                                <th>Nom</th>
                                                <td>{{ client.Nom }}</td>
                                            </tr>
                                            <tr>
                                                <th>Prenom</th>
                                                <td>{{ client.Prenom }}</td>
                                            </tr>
                                            <tr>
                                                <th>Telephone</th>
                                                <td>{{ client.Telephone }}</td>
                                            </tr>
                                            <tr>
                                                <th>NomUtilisateur</th>
                                                <td>{{ client.nomUtilisateur }}</td>
                                            </tr>
                                            <tr>
                                                <th>MotDePass</th>
                                                <td>{{ client.motDePass }}</td>
                                            </tr>
                                            <tr>
                                                <th>Cni</th>
                                                <td>{{ client.Cni }}</td>
                                            </tr>
                                            <tr>
                                                <th>PhotoIdentite</th>
                                                <td>{{ client.PhotoIdentite }}</td>
                                            </tr>
                                        </tbody>
                                    </table>

                                    

                                    <a href="{{ path('app_client_index') }}" class="btn btn-primary">back to list</a>

                                    <a href="{{ path('app_client_edit', {'id': client.id}) }}" class="btn btn-warning">edit</a>

                                    <a href="{{ path('client_supp', {'id': client.id}) }}" class="btn btn-danger">Supprimer</a>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    {% endblock %}
    {% block act1 %}{% endblock %}
{% block act2 %}{% endblock %}
{% block act3 %}{% endblock %}
{% block act4 %}active has-sub{% endblock %}
{% block act5 %}{% endblock %}
{% block username %}{{username}}{% endblock %}
{% block password %}{{username}}{% endblock %}
{% block Tel %}{{tel}}{% endblock %}